{
    "name": "andrey-helldar/pochta",
    "description": "Package for working with API Russian Post.",
    "type": "library",
    "license": "MIT",
    "homepage": "http://ai-rus.com",
    "keywords": [
        "pochta",
        "russian post"
    ],
    "support": {
        "issues": "https://github.com/andrey-helldar/Pochta/issues",
        "source": "https://github.com/andrey-helldar/Pochta"
    },
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "require": {
        "php": ">=5.6.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~1.11",
        "phpunit/phpunit": "~4.0|~5.0",
        "sllh/php-cs-fixer-styleci-bridge": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\Pochta\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit; ./vendor/bin/php-cs-fixer fix -v --diff --dry-run;",
        "phpunit": "./vendor/bin/phpunit;",
        "phpcs": "./vendor/bin/php-cs-fixer fix -v --diff --dry-run;"
    },
    "minimum-stability": "stable"
}
